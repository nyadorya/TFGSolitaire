<?xml version='1.0' encoding='utf-8'?>
<widget id="jp.echopeace.tfgsolitaire" version="0.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>TFGSolitaire</name>
    <description>
        TFGSolitaire is Solitaire game of Transformation Game.
    </description>
    <author email="kazu7108+tfg@gmail.com" href="http://">
        Kazushige Kodama
    </author>
    <content src="index.html" />
    <platform name="android">
        <icon density="ldpi" src="www/res/icon/android/android-chrome-36x36.png" />
        <icon density="mdpi" src="www/res/icon/android/android-chrome-48x48.png" />
        <icon density="hdpi" src="www/res/icon/android/android-chrome-72x72.png" />
        <icon density="xhdpi" src="www/res/icon/android/android-chrome-96x96.png" />
        <icon density="xxhdpi" src="www/res/icon/android/android-chrome-144x144.png" />
        <icon density="xxxhdpi" src="www/res/icon/android/android-chrome-192x192.png" />
    </platform>
    <platform name="ios">
        <icon height="120" platform="ios" src="www/res/icon/ios/icon-60@2x.png" width="120" />
        <icon height="180" platform="ios" src="www/res/icon/ios/icon-60@3x.png" width="180" />
        <icon height="76" platform="ios" src="www/res/icon/ios/icon-76.png" width="76" />
        <icon height="152" platform="ios" src="www/res/icon/ios/icon-76@2x.png" width="152" />
        <icon height="167" platform="ios" src="www/res/icon/ios/Icon-83.5@2x.png" width="167" />
        <icon height="29" platform="ios" src="www/res/icon/ios/icon-small.png" width="29" />
        <icon height="58" platform="ios" src="www/res/icon/ios/icon-small@2x.png" width="58" />
        <icon height="87" platform="ios" src="www/res/icon/ios/icon-small@3x.png" width="87" />
        <icon height="40" platform="ios" src="www/res/icon/ios/Icon-Small-40.png" width="40" />
        <icon height="80" platform="ios" src="www/res/icon/ios/Icon-Small-40@2x.png" width="80" />
        <icon height="120" platform="ios" src="www/res/icon/ios/Icon-Small-40@3x.png" width="120" />
        <icon height="20" platform="ios" src="www/res/icon/ios/iPad-20@1x.png" width="20" />
        <icon height="40" platform="ios" src="www/res/icon/ios/iPad-20@2x.png" width="40" />
        <icon height="40" platform="ios" src="www/res/icon/ios/iPhone-20@2x.png" width="40" />
        <icon height="60" platform="ios" src="www/res/icon/ios/iPhone-20@3x.png" width="60" />
    </platform>
    <access origin="*" />
    <access subdomains="true" uri="*" />
    <allow-intent href="*" />
    <allow-navigation href="*" />
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="orientation" value="all" />
        <access allows-arbitrary-loads-for-media="true" allows-arbitrary-loads-in-web-content="true" allows-local-networking="true" origin="*" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <preference name="Orientation" value="portrait" />
    <engine name="android" spec="~6.3.0" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.3" />
</widget>
